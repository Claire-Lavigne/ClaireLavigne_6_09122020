{
  "name": "clairelavigne_6_09122020",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "browserslist": "last 4 versions",
  "scripts": {
    "prebuild": "node -e \"require('fs').mkdirSync('public/css/prefixed',{recursive:true})\"",
    "build:sass": "sass --load-path=sass sass/main.scss public/css/style.src.css --style=compressed",
    "build:prefix": "postcss public/css/style.src.css --use autoprefixer -o public/css/prefixed/style.css",
    "build": "npm run prebuild && npm run build:sass && npm run build:prefix",
    "watch:sass": "sass --load-path=sass --watch sass/main.scss:public/css/style.src.css --style=compressed",
    "watch:prefix": "postcss public/css/style.src.css --use autoprefixer -o public/css/prefixed/style.css --watch",
    "start": "http-server ./ -a localhost -p 8080 -c-1",
    "dev": "npm run build && npm-run-all --parallel watch:sass watch:prefix start"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.22",
    "eslint": "^7.21.0",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.94.0"
  },
  "dependencies": {
    "live-server": "^1.2.1"
  }
}
